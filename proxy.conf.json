{
  "/api/chat": {
    "target": "http://localhost:8081",
    "secure": false,
    "changeOrigin": true,
    "logLevel": "debug",
    "headers": {
      "Authorization": "${req.headers.authorization}"
    }
  },
  "/api": {
    "target": "http://localhost:8080",
    "secure": false,
    "changeOrigin": true,
    "logLevel": "debug",
    "headers": {
      "Authorization": "${req.headers.authorization}"
    }
  }
}
